DB_HOST=localhost
DB_USER=family_album_user
DB_PASSWORD=root
DB_NAME=family_album
DB_PORT=3306

# 初期管理者ユーザー設定
# ADMIN_EMAIL: 管理者のメールアドレスを設定してください
ADMIN_EMAIL=your-family@example.com

# ADMIN_PASSWORD_HASH: 管理者パスワードのbcryptハッシュ値
# パスワード「admin123」のハッシュ例: $2b$12$PHDnGi0uURpMmQJiuVRAVuITyyRuiC2dEcH2d.USDCydAGCODo/Dq
# 本番環境では必ず強力なパスワードに変更してください
ADMIN_PASSWORD_HASH=your_bcrypt_hash_here

# JWTシークレットキー
# セキュリティのため、ランダムで十分に長い文字列を設定してください
JWT_SECRET_KEY=your_jwt_secret_key